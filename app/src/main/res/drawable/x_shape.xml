<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:drawable="@color/darkBlue" />

    <item
        android:left="16dp"
        android:right="16dp"
        android:top="16dp"
        android:bottom="16dp">
        <rotate android:fromDegrees="45" android:toDegrees="45">
            <shape android:shape="line">
                <stroke
                    android:width="32dp"
                    android:color="@color/white" />
            </shape>
        </rotate>
    </item>

    <item
        android:left="16dp"
        android:right="16dp"
        android:top="16dp"
        android:bottom="16dp">
        <rotate android:fromDegrees="-45" android:toDegrees="-45">
            <shape android:shape="line">
                <stroke
                    android:width="32dp"
                    android:color="@color/white" />
            </shape>
        </rotate>
    </item>
</layer-list>
